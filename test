<!DOCTYPE html>
<html>
<head>
  <title>Data dari Google Sheets</title>
</head>
<body>
  <h1>Data dari Google Sheets</h1>
  <div id="output"></div>

  <script>
    const sheetUrl = 'https://docs.google.com/spreadsheets/d/e/2PACX-1vS5Z7BbTgUC3J4n9LFYF3Uaqs7E853UW6LshGBwOnOCtMZD9rBv0UkgQ-AV_6f8h_2m90CYHwgIPHmP/pub?gid=1556757093&single=true&output=csv';

    fetch(sheetUrl)
      .then(response => response.text())
      .then(data => {
        const rows = data.split('\n');
        const output = document.getElementById('output');
        rows.forEach(row => {
          const cols = row.split(',');
          const p = document.createElement('p');
          p.textContent = cols.join(' | ');
          output.appendChild(p);
        });
      });
  </script>
</body>
</html>
